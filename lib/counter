:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x56131493ecf0 .scope module, "counter_tb" "counter_tb" 2 1;
 .timescale 0 0;
P_0x561314907440 .param/l "FREQ" 0 2 3, +C4<00000000000000001001110001000000>;
P_0x561314907480 .param/l "PERIOD" 0 2 4, +C4<00000000000000000000001111101000>;
P_0x5613149074c0 .param/l "WIDTH" 0 2 2, +C4<00000000000000000000000000010000>;
v0x561314950ec0_0 .var "clk", 0 0;
v0x561314950f90_0 .net "cnt", 15 0, v0x561314950c90_0;  1 drivers
v0x561314951060_0 .var "rstn", 0 0;
S_0x56131493efa0 .scope module, "c0" "counter" 2 10, 3 1 0, S_0x56131493ecf0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rstn";
    .port_info 2 /OUTPUT 16 "cnt";
P_0x56131493f450 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000010000>;
v0x56131492c430_0 .net "clk", 0 0, v0x561314950ec0_0;  1 drivers
v0x561314950c90_0 .var "cnt", 15 0;
v0x561314950d70_0 .net "rstn", 0 0, v0x561314951060_0;  1 drivers
E_0x56131493e060 .event posedge, v0x56131492c430_0;
    .scope S_0x56131493efa0;
T_0 ;
    %wait E_0x56131493e060;
    %load/vec4 v0x561314950d70_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x561314950c90_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x561314950c90_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x561314950c90_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x56131493ecf0;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x56131493ecf0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x56131493ecf0;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x561314950ec0_0;
    %inv;
    %assign/vec4 v0x561314950ec0_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x56131493ecf0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561314950ec0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561314951060_0, 0;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561314951060_0, 0;
    %delay 1000, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
