#! /usr/local/bin/vvp
:ivl_version "12.0 (devel)" "(s20150603-955-gd6e01d0c)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x561da5d0b810 .scope module, "counter_tb" "counter_tb" 2 3;
 .timescale -9 -12;
P_0x561da5cd2f50 .param/l "FREQ" 0 2 5, +C4<00000000000000001001110001000000>;
P_0x561da5cd2f90 .param/l "PERIOD" 0 2 6, +C4<00111011100110101100101000000000>;
P_0x561da5cd2fd0 .param/l "WIDTH" 0 2 4, +C4<00000000000000000000000000010000>;
v0x561da5d1e0f0_0 .var "clk", 0 0;
v0x561da5d1e1c0_0 .net "cnt", 15 0, v0x561da5d1dec0_0;  1 drivers
v0x561da5d1e290_0 .var "rst_n", 0 0;
S_0x561da5cd43b0 .scope module, "c0" "counter" 2 12, 3 1 0, S_0x561da5d0b810;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 16 "cnt";
P_0x561da5cd4590 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000010000>;
v0x561da5d0c0c0_0 .net "clk", 0 0, v0x561da5d1e0f0_0;  1 drivers
v0x561da5d1dec0_0 .var "cnt", 15 0;
v0x561da5d1dfa0_0 .net "rst_n", 0 0, v0x561da5d1e290_0;  1 drivers
E_0x561da5d0abe0 .event posedge, v0x561da5d0c0c0_0;
    .scope S_0x561da5cd43b0;
T_0 ;
    %wait E_0x561da5d0abe0;
    %load/vec4 v0x561da5d1dfa0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x561da5d1dec0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x561da5d1dec0_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x561da5d1dec0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x561da5d0b810;
T_1 ;
    %vpi_call 2 16 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x561da5d0b810 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x561da5d0b810;
T_2 ;
    %delay 12500000, 0;
    %load/vec4 v0x561da5d1e0f0_0;
    %inv;
    %assign/vec4 v0x561da5d1e0f0_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x561da5d0b810;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561da5d1e0f0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561da5d1e290_0, 0;
    %delay 25000000, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561da5d1e290_0, 0;
    %delay 3567587328, 232;
    %vpi_call 2 30 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "counter_tb.v";
    "counter.v";
